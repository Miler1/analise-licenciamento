.btn-group(data-toggle="buttons")

    
    label.btn.btn-plain(for="{{$ctrl.nome}}_valido", title="Emitir", ng-class="{'ativo': $ctrl.analiseLicenca.emitir}", ng-click="$ctrl.setEmitir(true)")
        
        input(id="{{$ctrl.nome}}_valido",type="radio", name="{{$ctrl.nome}}", ng-value="true", value="true", ng-model="$ctrl.analiseLicenca.emitir")
        
        span.glyphicon.glyphicon-ok.checked.valido
        span.glyphicon.glyphicon-ok.unchecked.nao-marcado

    label.btn.btn-plain(for="{{$ctrl.nome}}_invalido", title="NÃ£o emitir", ng-class="{'ativo': $ctrl.analiseLicenca.emitir === false}", ng-click="$ctrl.setEmitir(false)")
        
        input(id="{{$ctrl.nome}}_invalido",type="radio", name="{{$ctrl.nome}}", ng-value="false", value="false", ng-model="$ctrl.analiseLicenca.emitir")
                       
        span.glyphicon.glyphicon-remove.checked.invalido 
        span.glyphicon.glyphicon-remove.unchecked.nao-marcado
