		link(href="./summernote/summernote.css", rel="stylesheet")

	body(id="ng-app", ng-app="licenciamento", ng-controller="AppController", ng-cloak)

		div#loader

			div#wrap

			div(ng-if='!isRoute("/parecer-orgao") && !isRoute("/parecer-juridico")')

				include layout/header
				include layout/menu
				section.container.main-container

					include components/breadcrumb/breadcrumb

					div(growl inline="true")

					div

						aside.col-xs-3.main-container__menu
							menu-principal(itens="itensMenuPrincipal")

						div.col-xs-9.main-container__views
							div(ng-view)

				//- include layout/footer

			div(ng-if='isRoute("/parecer-orgao")')

				include layout/header
				div.col-xs-12.main-container__parecer-orgao
					section.container.main-container-parecer-orgao
						include features/parecerOrgao/parecerOrgao
			
			div(ng-if='isRoute("/parecer-juridico")')

				include layout/header
				div.col-xs-12.main-container__parecer-juridico
					section.container.main-container-parecer-juridico
						include features/parecerJuridico/parecerJuridico

