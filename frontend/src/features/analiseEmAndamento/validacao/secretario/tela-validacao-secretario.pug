div.row

	.col-xs-12.col-md-8
			div.row.label-cabecalho-protocolos
				label.col-sm-4 Licença Solicitada:
				div.col-sm-8
					span {{ validacaoAnaliseSecretario.dadosProcesso.caracterizacao.tipoLicenca.nome }}
			
			div.row.label-cabecalho-protocolos
				label.col-sm-4 Tipologia:
				span.col-sm-8
					div {{ validacaoAnaliseSecretario.dadosProcesso.caracterizacao.atividadesCaracterizacao[0].atividade.tipologia.nome }}

			div.row.label-cabecalho-protocolos
				label.col-sm-4 Atividade(s) Licenciada(s):
				.col-sm-8
					div
						span(ng-repeat="atividadeCaracterizacao in validacaoAnaliseSecretario.dadosProcesso.caracterizacao.atividadesCaracterizacao") {{atividadeCaracterizacao.atividade.nome}}
							div(ng-if="!validacaoAnaliseSecretario.dadosProcesso.cpfEmpreendimento")
								div(ng-repeat='atividadeCnae in atividadeCaracterizacao.atividadesCnae')
									ul CNAE: {{atividadeCnae.codigo}} - {{atividadeCnae.nome}}
							div(ng-if="validacaoAnaliseSecretario.dadosProcesso.cpfEmpreendimento")
								div(ng-repeat='atividadeCnae in atividadeCaracterizacao.atividadesCnae')
									ul CNAE: {{atividadeCnae.nome}}
			.col-xs-12.col-md-8
				div.row
					h4 Histórico do protocolo
					label Data: &nbsp;
					span {{ validacaoAnaliseSecretario.dateUtil.formatarData(validacaoAnaliseSecretario.dadosProcesso.analise.dataCadastro) }}

					div.panel.panel-success
						div.panel-body
							| Análise do protocolo em dias
						div.panel-footer
							.row
								.col-xs-4.col-md-4.text-center
									span Análise GEO
										div(ng-class="{'error-message': validacaoAnaliseSecretario.dateUtil.verificaPrazoMinimo(validacaoAnaliseSecretario.dadosProcesso.analise.diasAnalise.qtdeDiasGeo, validacaoAnaliseSecretario.PrazoAnalise.ANALISE_GEO)}")
											b {{ validacaoAnaliseSecretario.dadosProcesso.analise.diasAnalise.qtdeDiasGeo }}
								.col-xs-4.col-md-4.text-center
									span Análise técnica
										div(ng-class="{'error-message': validacaoAnaliseSecretario.dateUtil.verificaPrazoMinimo(validacaoAnaliseSecretario.dadosProcesso.analise.diasAnalise.qtdeDiasTecnica, validacaoAnaliseSecretario.PrazoAnalise.ANALISE_TECNICA)}")
											b {{ validacaoAnaliseSecretario.dadosProcesso.analise.diasAnalise.qtdeDiasTecnica ? validacaoAnaliseSecretario.dadosProcesso.analise.diasAnalise.qtdeDiasTecnica : 0 }}
								.col-xs-4.col-md-4.text-center
									span Análise total
										div
											b {{ validacaoAnaliseSecretario.dadosProcesso.analise.diasAnalise.qtdeDiasAnalise }}

	.col-xs-12.col-md-4
		div.panel.panel-success
			div.panel-body
				| Parâmetro
			div#panelResumo.panel-footer.allign-left
				.media-div
					.media-context
						div
							div(ng-repeat="atividadeCaracterizacao in validacaoAnaliseSecretario.dadosProcesso.caracterizacao.atividadesCaracterizacao")
								h5.info {{atividadeCaracterizacao.atividade.nome}}
								div(ng-repeat="atividadeCaracterizacaoParametro in atividadeCaracterizacao.atividadeCaracterizacaoParametros")
									ul(style="margin-left:25px")
										li
											span {{atividadeCaracterizacao.atividade.parametros[$index].codigo}} - {{atividadeCaracterizacao.atividade.parametros[$index].nome}} : {{atividadeCaracterizacaoParametro.valorParametro}}

		div.panel.panel-success
			div.panel-body
				| Porte - Potencial Poluidor/Degradador
			div.panel-footer.allign-left
				.media-div
					.media-context
						div
							div {{validacaoAnaliseSecretario.dadosProcesso.caracterizacao.atividadeCaracterizacaoMaiorPotencialPoluidorEPorte.porteEmpreendimento.nome}} - {{validacaoAnaliseSecretario.dadosProcesso.caracterizacao.atividadeCaracterizacaoMaiorPotencialPoluidorEPorte.atividade.potencialPoluidor.nome}}

div.row
		.col-xs-12
				h4 Tramitação do protocolo

				div(uib-accordion class="accordion")
					div(uib-accordion-group class="panel-default" is-open="validacaoAnaliseSecretario.abreTramitacaoProcessoAtual")
						uib-accordion-heading(ng-click="validacaoAnaliseSecretario.abreTramitacaoProcessoAtual=!validacaoAnaliseSecretario.abreTramitacaoProcessoAtual")
							div Tramitação do Protocolo Atual
								i(class="pull-right glyphicon" ng-class="{'glyphicon-chevron-down': validacaoAnaliseSecretario.abreTramitacaoProcessoAtual, 'glyphicon-chevron-right': !validacaoAnaliseSecretario.abreTramitacaoProcessoAtual}")

						div
							table.table.table-striped.table-condensed

								thead
									tr
										th Data Entrada
										th Data Saída
										th Ação
										th Status
										th Tempo do Status
										th Responsável
										th Observação

								tbody
									tr(ng-repeat="tramitacao in validacaoAnaliseSecretario.dadosProcesso.historicoTramitacao")
										td.col-md-1 {{tramitacao.dataInicial ? tramitacao.dataInicial : '-'}}
										td.col-md-1 {{tramitacao.dataFinal ? tramitacao.dataFinal : '-'}}
										td.col-md-2 {{tramitacao.nomeAcao ? tramitacao.nomeAcao : '-'}}
										td.col-md-2 {{tramitacao.nomeCondicaoFinal ? tramitacao.nomeCondicaoFinal : '-'}}
										td.col-md-1 {{tramitacao.tempoPermanencia ? tramitacao.tempoPermanencia : '-'}}
										td.col-md-3 {{tramitacao.nomeUsuarioExecutor ? tramitacao.nomeUsuarioExecutor : '-'}}
										td.col-md-1 
											button.btn.btn-default.pull-center.btn-visualizar-justificativa(ng-if="validacaoAnaliseSecretario.validaJustificativas(tramitacao)",
																											ng-click="validacaoAnaliseSecretario.visualizarJustificativas(validacaoAnaliseSecretario.dadosProcesso.idProcesso, tramitacao)")
												i.glyphicon.glyphicon-list-alt
div.row
		.col-xs-12
				h4 Validação da análise

				div(uib-accordion class="accordion")
					div(uib-accordion-group class="panel-default" is-open="validacaoAnaliseSecretario.abreConsideracaoDiretor")
						uib-accordion-heading(ng-click="validacaoAnaliseSecretario.abreConsideracaoDiretor=!validacaoAnaliseSecretario.abreConsideracaoDiretor")
							div Consideração do diretor técnico
								i(class="pull-right glyphicon" ng-class="{'glyphicon-chevron-down': validacaoAnaliseSecretario.abreConsideracaoDiretor, 'glyphicon-chevron-right': !validacaoAnaliseSecretario.abreConsideracaoDiretor}")

						div
							label.label-documento-minuta Parecer do Diretor Técnico:
							br
							p(ng-bind-html="validacaoAnaliseSecretario.parecerDiretor.parecer")
						div
							label.label-documento-minuta(style="margin-bottom:5px;") Documentos de minuta:
							
							table.table.table-condensed.margin-grid-licenca(ng-if="validacaoAnaliseSecretario.parecerTecnico.documentoMinuta != null ")

								thead

									tr
										th.col-md-10 Minutas
										th.col-md-2.text-right Ação

								tbody
									tr  
										td.col-md-10 {{ validacaoAnaliseSecretario.parecerTecnico.documentoMinuta.nomeDoArquivo }}
										td.col-md-2.text-right
											a.d-inline.margin-right-xs.aba-clicavel(style='cursor: pointer;',title="Visualizar anexo", ng-click="validacaoAnaliseSecretario.baixarDocumento(validacaoAnaliseSecretario.analiseTecnica, validacaoAnaliseSecretario.tipoDocumento.DOCUMENTO_MINUTA)")
												i.glyphicon.glyphicon-eye-open
									

							div.info-sem-dados-para-exibir(ng-if=" validacaoAnaliseSecretario.parecerTecnico.documentoMinuta == null ")
								i Não foi gerado nenhum documento.

						label.label-documento-minuta Validação da Licença
						table.table.table-striped.table-condensed.margin-grid-licenca(style="margin-bottom:10px;")
							thead
								tr
									th
									th.col-md-5.text-left Licença
									th.col-md-4.text-left Porte e Potencial Poluidor
									th.col-md-2.text-left Validade
									th.col-md-1.text-right Ação
							tbody
								tr
									td
										i.glyphicon.glyphicon-ok-sign.nao-tem-inconsistencia(ng-if="validacaoAnaliseSecretario.parecerDiretor.tipoResultadoAnalise.id === validacaoAnaliseSecretario.tiposResultadoAnalise.ANALISE_APROVADA")
										i.glyphicon.glyphicon-remove-sign.temInconsistencia(ng-if="validacaoAnaliseSecretario.parecerDiretor.tipoResultadoAnalise.id === validacaoAnaliseSecretario.tiposResultadoAnalise.ANALISE_NAO_APROVADA")
									td {{ validacaoAnaliseSecretario.dadosProcesso.caracterizacao.tipoLicenca.nome }}
									td {{ validacaoAnaliseSecretario.dadosProcesso.caracterizacao.atividadeCaracterizacaoMaiorPotencialPoluidorEPorte.porteEmpreendimento.nome }} - {{ validacaoAnaliseSecretario.dadosProcesso.caracterizacao.atividadeCaracterizacaoMaiorPotencialPoluidorEPorte.atividade.potencialPoluidor.nome }}
									td(style="padding-left: 32px;",ng-if="validacaoAnaliseSecretario.parecerTecnico.tipoResultadoAnalise.id === validacaoAnaliseSecretario.tiposResultadoAnalise.INDEFERIDO || validacaoAnaliseSecretario.possuiValidade === false") -
									td(ng-if="validacaoAnaliseSecretario.parecerTecnico.tipoResultadoAnalise.id === validacaoAnaliseSecretario.tiposResultadoAnalise.DEFERIDO && validacaoAnaliseSecretario.possuiValidade === true") {{ validacaoAnaliseSecretario.parecerTecnico.validadePermitida }} anos
									td.text-right
										a.d-inline.margin-right-xs.aba-clicavel(style='cursor: pointer;',title="Visualizar licença", ng-click="validacaoAnaliseSecretario.openModalLicenca(validacaoAnaliseSecretario.parecerTecnico, validacaoAnaliseSecretario.dadosProcesso, validacaoAnaliseSecretario.analiseTecnica)")
											i.glyphicon.glyphicon-eye-open
						div.col-md-12
							.col-md-2
							div.col-md-5
								i.glyphicon.glyphicon-ok-sign.nao-tem-inconsistencia
								span.label-legenda-secretario &nbsp;Licença APROVADA
							div.col-md-4
								i.glyphicon.glyphicon-remove-sign.temInconsistencia 
								span.label-legenda-secretario &nbsp;Licença NÃO APROVADA
						br
						br
						hr
						#conclusao
							label.obrigatorio Análise final dos pareceres:
								div.pull-left.col-md-10.posicao-principal
									form.pull-left.posicao-conteudo-secretario
										label.opDla.radio-inline.posicao-primeiroItem-diretor
											input(type='radio', name='analiseFinalRadio', ng-model='validacaoAnaliseSecretario.idTipoResultadoAnalise', value="{{validacaoAnaliseSecretario.tiposResultadoAnalise.ANALISE_APROVADA}}")
											| APROVAR LICENCIAMENTO
										label.opDla.radio-inline(style="margin-left:150px;")
											input(type='radio', name='analiseFinalRadio', ng-model='validacaoAnaliseSecretario.idTipoResultadoAnalise', value="{{validacaoAnaliseSecretario.tiposResultadoAnalise.ANALISE_NAO_APROVADA}}")
											| NÃO APROVAR LICENCIAMENTO

						div(ng-if="validacaoAnaliseSecretario.idTipoResultadoAnalise === validacaoAnaliseSecretario.tiposResultadoAnalise.ANALISE_APROVADA.toString()")
							label.obrigatorio.label-documento-minuta Despacho:
							br
							div(ng-class="{'error-summernote': validacaoAnaliseSecretario.errors.despacho === true}")
								summernote.form-control(id="conclusao",
											on-paste="snPaste(evt)",
											name="conclusao"
											lang="pt-BR",
											height="200",
											ng-model="validacaoAnaliseSecretario.parecerSecretario",
											config="optionsText",
											class="editable",
											contenteditable="true")

						div(ng-if="validacaoAnaliseSecretario.idTipoResultadoAnalise === validacaoAnaliseSecretario.tiposResultadoAnalise.ANALISE_NAO_APROVADA.toString()")
							label.obrigatorio.label-documento-minuta Justificativa:
							br
							div(ng-class="{'error-summernote': validacaoAnaliseSecretario.errors.despacho === true}")
								summernote.form-control(id="conclusao",
											on-paste="snPaste(evt)",
											name="conclusao"
											lang="pt-BR",
											height="200",
											ng-model="validacaoAnaliseSecretario.parecerSecretario",
											config="optionsText",
											class="editable",
											contenteditable="true")        