include ../../../../layout/components/panel
include ../../../../features/resumoProcesso/resumo-processo
include ../../../../layout/components/inputErrors

+resumo-processo('validacaoAnalisePresidente.analiseTecnica.analise.processo','validacaoAnalisePresidente.exibirDadosProcesso()','validacaoAnalisePresidente.titulo')

section.margin-bottom-md

	.row
		div(style="padding:15px;")
			div.modal-body

			include tela-validacao-presidente

section(style="padding-top:20px;")

	button.btn.btn-default.pull-left.posicao-cancelar-gerente(title='Cancelar análise',
                                    ng-bootbox-title='Confirmação',
                                    ng-bootbox-confirm='Deseja realmente cancelar a análise ?',
                                    ng-bootbox-confirm-action="validacaoAnalisePresidente.cancelar()")
		i.glyphicon.glyphicon-ban-circle.glyphicon--with-right-margin
		| Cancelar
	
	button.btn.btn-success.posicao-avancar-concluir-gerente(ng-click="validacaoAnalisePresidente.concluir()")
		i.glyphicon.glyphicon-check.glyphicon--with-right-margin
		| Concluir

	button.btn.btn-default.posicao-voltar-gerente(ng-click="validacaoAnalisePresidente.voltarEtapaAnterior()")
		i.glyphicon.glyphicon-arrow-left
		| Voltar
		
	button.btn.btn-success.posicao-avancar-gerente(ng-click="validacaoAnalisePresidente.avancarProximaEtapa()")
		i.glyphicon.glyphicon-arrow-right
		| &nbsp;Avançar                
