    //-             select.form-control(ng-model="$ctrl.filtro.idAtividadeEmpreendimento", name="selectAtividade",
    //-                                 ng-options="atividade.id as atividade.nome for atividade in $ctrl.atividades | orderBy:'nome' track by atividade.id")
    //-                 option(value="") Selecione a atividade do empreendimento


	.row.form-group

		.col-md-6.col-sm-6
			label.control-label Tipologia: *
			select.form-control(ng-model="cadastroProcessoManejoController.tipologias", name="selectTipologia",
							ng-options="tipologia.id as tipologia.nome for tipologia in cadastroProcessoManejoController.tipologias | orderBy:'nome' track by tipologia.id", required)
				option(value="") Selecione a tipologia do processo

		.col-md-6.col-sm-6
			label.control-label Atividade: *
			select.form-control(ng-model="cadastroProcessoManejoController.atividades", name="selectMunicipio",
								ng-options="", required)
				option(value="") Selecione a atividade do processo

	.row.form-group

		.col-md-6.col-sm-6
			label.control-label Município: *
			select.form-control(ng-model="cadastroProcessoManejoController.municipios", name="selectMunicipio",
								ng-options="", required)
				option(value="") Selecione o municipio

		.col-md-6.col-sm-6
			label.control-label Licença:
			.col-md-12.div-checkbox-manejo
				input.checkbox-manejo(type='checkbox', placeholder='APAT', disabled="true", ng-model='cadastroProcessoManejoController.licencas', ng-click="")
				|  APAT
				input.checkbox-manejo(type='checkbox', placeholder='AUTEF', disabled="true", ng-model='cadastroProcessoManejoController.licencas', ng-click="")
				|  AUTEF
				input.checkbox-manejo(type='checkbox', placeholder='LAR', disabled="true", ng-model='cadastroProcessoManejoController.licencas', ng-click="")
				|  LAR

	hr

	div.form-group

		label.required.control-label(for='cpf') Informe o CAR: *

		div.input-group

			input.form-control(type='text',,
								ng-model='cadastroProcessoManejoController.car.numero',
								enter='',
								placeholder='Informe o CAR do processo.',
								name='car',
							)

			span.input-group-btn
				button.btn.btn-default(type='button', ng-click='', ng-disabled="")
					span.glyphicon.glyphicon-search

	.row
		label.subtitulo.col-sm-3 Proprietário
	.row
		label.col-sm-3 CPF/CNPJ:
		.col-sm-3
			span {{cadastroProcessoManejoController.car.proprietario.cpfCnpj}}
		label.col-sm-3 Nome:
		.col-sm-3
			span {{cadastroProcessoManejoController.car.proprietario.nome}}
	.row
		label.col-sm-3 Endereço:
		.col-sm-3
			span {{cadastroProcessoManejoController.car.proprietario.endereco}}
		label.col-sm-3 Município / UF:
		.col-sm-3
			span {{cadastroProcessoManejoController.car.proprietario.municipio}}
