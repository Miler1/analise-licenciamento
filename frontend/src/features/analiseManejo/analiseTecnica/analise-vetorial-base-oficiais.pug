include ./observacoes

h5 Dados analisados da AMF pelo GEOTEC

div
	blockquote.analise-manejo
		table.table.table-striped.table-condensed

			thead
				tr
					th.text-center(colspan="2") Vetores
					th.text-center.coluna-dupla-divisao(colspan="2") Propriedade
					th.text-center.coluna-dupla-divisao(colspan="2") AMF
					th.text-center.coluna-dupla.coluna-dupla-divisao(rowspan="2") Observação
				tr
					th.text-center Tipo
					th.text-center Nome
					th.text-center Distância (km)
					th.text-center Sobreposição (ha)
					th.text-center Distância (km)
					th.text-center Sobreposição (ha)

			tbody
				tr(ng-repeat="analise in analiseTecnicaManejo.analiseTecnica.analisesVetorial")
					td.col-md-1(style='max-width: 100px;') {{analise.tipo || '-'}}
					td.col-md-1 {{analise.nome || '-'}}
					td.col-md-1(style='max-width: 90px; word-wrap: break-word;') {{analise.distanciaPropriedade || '-'}}
					td.col-md-1(style='max-width: 90px; word-wrap: break-word;') {{analise.sobreposicaoPropriedade || '-'}}
					td.col-md-1(style='max-width: 90px; word-wrap: break-word;') {{analise.distanciaAmf || '-'}}
					td.col-md-1(style='max-width: 90px; word-wrap: break-word;') {{analise.sobreposicaoAmf || '-'}}
					td.col-md-1(style='min-width: 140px;') {{analise.observacao || '-'}}


div
	+observacoes('analiseTecnicaManejo.analiseTecnica.observacoesAnaliseVetorial')