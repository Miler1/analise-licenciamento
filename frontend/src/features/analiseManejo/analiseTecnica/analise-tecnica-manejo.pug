h4 Parecer Técnico

hr
div.row#dadosProcesso(ng-init="analiseTecnicaManejo.init()")
	.col-xs-12.col-md-8
		blockquote.success

			div.row
				label.col-sm-4 Processo:
				span.col-sm-8 {{analiseTecnicaManejo.analiseTecnica.processoManejo.numeroProcesso}}

			div.row
				label.col-sm-4 CPF/CNPJ:
				span.col-sm-8 {{analiseTecnicaManejo.analiseTecnica.processoManejo.cpfCnpj | brCpfCnpj}}

			div.row
				label.col-sm-4 Nome/Razão Social:
				span.col-sm-8 {{analiseTecnicaManejo.analiseTecnica.processoManejo.denominacaoEmpreendimentoSimlam}}

			div.row
				label.col-sm-4 Município:
				span.col-sm-8 {{analiseTecnicaManejo.analiseTecnica.processoManejo.nomeMunicipioSimlam}}

div

	ul.nav.nav-tabs(role="tablist")
		li.active(role="presentation")
			a(data-toggle="tab", data-target="#dados-imovel") Imóvel
		li(role="tab")
			a(data-toggle="tab", data-target="#metodos-base-vetorial") Metadados
		li(role="tab")
			a(data-toggle="tab", data-target="#analise-vetorial-base-oficiais") Análise Vetorial
		li(role="tab")
			a(data-toggle="tab", data-target="#analise-temporal-imagens-satelite") Análise Temporal
		li(role="tab")
			a(data-toggle="tab", data-target="#insumos-utilizados-analise-temporal") Insumos
		li(role="tab")
			a(data-toggle="tab", data-target="#calculo-ndfi") NDFI
		li(role="tab")
			a(data-toggle="tab", data-target="#calculo-area-efetiva-manejo") Área Efetiva
		li(role="tab")
			a(data-toggle="tab", data-target="#detalhamento-area-efetiva-manejo") Detalhamento
		li(role="tab")
			a(data-toggle="tab", data-target="#consideracoes") Considerações
		li(role="tab")
			a(data-toggle="tab", data-target="#conclusao") Conclusão

	.tab-content

		#dados-imovel.tab-pane.fade.in.active
			include dados-imovel

		#metodos-base-vetorial.tab-pane.fade
			include metodos-base-vetorial

		#analise-vetorial-base-oficiais.tab-pane.fade
			include analise-vetorial-base-oficiais

		#analise-temporal-imagens-satelite.tab-pane.fade
			include analise-temporal-imagens-satelite

		#insumos-utilizados-analise-temporal.tab-pane.fade
			include insumos-utilizados-analise-temporal

		#calculo-ndfi.tab-pane.fade
			include calculo-ndfi

		#calculo-area-efetiva-manejo.tab-pane.fade
			include calculo-area-efetiva-manejo

		#detalhamento-area-efetiva-manejo.tab-pane.fade
			include detalhamento-area-efetiva-manejo

		#consideracoes.tab-pane.fade
			include consideracoes

		#conclusao.tab-pane.fade
			include conclusao
